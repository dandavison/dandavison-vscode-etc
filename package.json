{
  "name": "vscode-etc",
  "displayName": "vscode-etc",
  "description": "Miscellaneous modifications",
  "publisher": "dandavison",
  "repository": {
    "url": "https://github.com/dandavison/vscode-etc",
    "type": "git"
  },
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:etc.copyGithubUrl",
    "onCommand:etc.emacsclient",
    "onCommand:etc.magitStatus",
    "onCommand:etc.openFolders"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "etc.copyGithubUrl",
        "title": "Copy GitHub URL"
      },
      {
        "command": "etc.copyGithubMarkdownUrl",
        "title": "Copy GitHub markdown URL"
      },
      {
        "command": "etc.emacsclient",
        "title": "Emacs"
      },
      {
        "command": "etc.magitStatus",
        "title": "Emacs Magit Status"
      },
      {
        "command": "etc.openFolders",
        "title": "Open Folders"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.9.5"
  }
}
